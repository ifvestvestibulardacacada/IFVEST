<script>
    // script ejs nao funciona em arquivos .js
    const ContentManager = (() => {
        const questoes = <%- JSON.stringify(questoes) %>;
        const id_simulado = <%- typeof simulado !== 'undefined' && simulado !== null ? JSON.stringify(simulado.id_simulado) : '0' %>;

        return {
            getQuestoes: () => [...questoes], // cópia segura do array
            getIdSimulado: () => id_simulado, // retorna null se não existir
            // utilitário para checar existência
        };
    })();

    window.ContentManager = ContentManager;

</script>