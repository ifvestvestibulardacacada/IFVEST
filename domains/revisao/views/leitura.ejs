    onload="renderMathInElement(document.body);"></script>
<title><%=conteudo.nome%> - Módulo de Revisão - IFVest</title>
<div class="container">
    <%if (conteudo) {%>
        <h1 class="text-center mt-5"><%=conteudo.nome%></h1>
        <h2 class="text-center mb-5 fs-5">Material de apoio de <%=conteudo.Assunto.nome%></h2>
        <span><i class='bx  bx-chevron-left'></i><a href="/revisao/busca/<%=conteudo.Assunto.id_assunto%>">Voltar para <%=conteudo.Assunto.nome%></a></span>
        <hr>
        <div class="markdown-area">
            <div class="row d-flex justify-content-center">
                <div class="col-lg-8">
                    <%-conteudo_markdown%>
                </div>
            </div>
        </div>
    <%} else {%>
        Ops, nao achei nada aqui :(
    <%}%>

</div>